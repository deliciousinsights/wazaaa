{
  "name": "wazaaa",
  "version": "1.0.0",
  "description": "Demo app used in Delicious Insightsâ€™ Node.js training course",
  "private": true,
  "engines": {
    "node": ">= 8",
    "npm": ">= 5"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --mode=production --progress --config webpack/webpack.config.js",
    "dev:debug": "nodemon --inspect src/boot.js",
    "dev:server": "nodemon src/boot.js",
    "dev:client": "webpack --mode=development --progress --watch --config webpack/webpack.config.js",
    "dev:test": "npm test -- --watch",
    "doc": "rimraf doc && npx groc",
    "fixtures": "mongo wazaaa mongo-scripts/*",
    "fixtures:win": "mongo.exe wazaaa mongo-scripts\\entries.js mongo-scripts\\users.js",
    "lint": "eslint .",
    "postinstall": "npm run build",
    "start": "node src/boot.js",
    "test": "cross-env NODE_ENV=test jest",
    "test:cov": "npm test -- --coverage"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      "stage-3"
    ]
  },
  "eslintConfig": {
    "extends": [
      "standard",
      "prettier",
      "plugin:import/errors"
    ],
    "plugins": [
      "prettier",
      "import"
    ],
    "parser": "babel-eslint",
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "arrowParens": "always",
          "singleQuote": true,
          "trailingComma": "es5",
          "semi": false
        }
      ],
      "no-irregular-whitespace": 0
    },
    "env": {
      "browser": true,
      "commonjs": true,
      "es6": true,
      "jest": true,
      "node": true
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "notify": true
  },
  "keywords": [
    "node",
    "express",
    "connect",
    "passport",
    "social",
    "demo",
    "training",
    "pug",
    "webpack",
    "jest",
    "tests"
  ],
  "author": "Christophe Porteneuve <christophe@delicious-insights.com> (http://delicious-insights.com)",
  "license": "No-License (see LICENSE file)",
  "dependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-3": "^6.22.0",
    "body-parser": "^1.18.3",
    "bootstrap": "^3.3.7",
    "chalk": "^2.4.1",
    "clean-webpack-plugin": "^0.1.19",
    "connect-flash": "^0.1.1",
    "cookie-session": "^2.0.0-beta.3",
    "cross-env": "^5.1.6",
    "css-loader": "^0.28.11",
    "csurf": "^1.9.0",
    "errorhandler": "^1.5.0",
    "eslint-loader": "^2.0.0",
    "express": "^4.16.3",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "hard-source-webpack-plugin": "^0.7.2",
    "image-webpack-loader": "^4.2.0",
    "isomorphic-fetch": "^2.2.1",
    "jquery": "^3.3.1",
    "method-override": "^2.3.10",
    "moment": "^2.22.1",
    "mongoose": "^5.1.3",
    "morgan": "^1.9.0",
    "passport": "^0.4.0",
    "passport-facebook": "^2.1.1",
    "passport-twitter": "^1.0.2",
    "postcss-cssnext": "^3.1.0",
    "postcss-loader": "^2.1.5",
    "pug": "^2.0.3",
    "pug-loader": "^2.4.0",
    "rimraf": "^2.6.2",
    "select2": "^4.0.6-rc.1",
    "select2-bootstrap-theme": "0.1.0-beta.10",
    "socket.io": "^2.1.1",
    "style-loader": "^0.21.0",
    "stylus": "^0.54.2",
    "stylus-loader": "^3.0.2",
    "svg-url-loader": "^2.3.2",
    "underscore": "^1.9.0",
    "unfluff": "^3.0.1",
    "url-loader": "^1.0.1",
    "webpack": "^4.10.0",
    "webpack-assets-manifest": "^3.0.1",
    "webpack-cli": "^2.1.4",
    "webpack-merge": "^4.1.2"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.3",
    "bson-objectid": "^1.2.3",
    "chai": "^4.1.2",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "faker": "^4.1.0",
    "jest": "^23.0.1",
    "nock": "^9.2.6",
    "nodemon": "^1.17.5",
    "prettier-eslint": "^8.8.1",
    "sinon": "^5.0.10",
    "sinon-chai": "^3.1.0",
    "supertest": "^3.1.0",
    "webpack-dashboard": "^2.0.0"
  }
}
